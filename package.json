{
  "name": "auth-service",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:sudoga/auth-service.git",
  "author": "sudoga",
  "license": "MIT",
  "scripts": {
    "generate": "npx prisma generate",
    "dev": "NODE_ENV=development tsnd --transpile-only ./src",
    "test": "NODE_ENV=test tsnd --transpile-only ./src",
    "tests": "dotenv -e .env.test jest",
    "prod": "NODE_ENV=production tsnd --transpile-only ./src",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev --name users",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name users",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate dev --name users"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.11",
    "dotenv-cli": "^5.0.0",
    "jest": "^27.5.1",
    "prisma": "^3.9.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@prisma/client": "^3.9.1",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1"
  }
